<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView0"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:text=""
        android:background="@drawable/background_rect_white_white"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="0dp"
        android:layout_marginStart="0dp" />


    <TextView
        android:id="@+id/textview1"
        android:layout_width="320dp"
        android:layout_height="48dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"

        android:background="@drawable/border_round_blue_white"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <EditText
        android:id="@+id/start_station"
        android:layout_width="270dp"
        android:layout_height="36dp"
        android:layout_marginStart="35dp"
        android:layout_marginTop="25dp"

        android:singleLine="true"
        android:imeOptions="actionDone"

        android:background="@color/white"
        android:hint="출발역을 입력하세요."
        android:textColor="#000000"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <TextView
        android:id="@+id/textview2"
        android:layout_width="320dp"
        android:layout_height="48dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="75dp"

        android:background="@drawable/border_round_blue_white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <EditText
        android:id="@+id/destination_station"
        android:layout_width="270dp"
        android:layout_height="36dp"
        android:layout_marginStart="35dp"
        android:layout_marginTop="80dp"

        android:singleLine="true"
        android:imeOptions="actionDone"

        android:background="@color/white"
        android:hint="도착역을 입력하세요."
        android:textColor="#000000"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <Button
        android:id="@+id/find_road"
        android:layout_width="48dp"
        android:layout_height="103dp"
        android:layout_marginStart="350dp"
        android:layout_marginTop="20dp"

        android:background="@drawable/border_round_blue_white"
        android:text="길찾기"
        android:textColor="#000000"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>


    <Button
        android:id="@+id/subwayBtn"
        android:layout_width="170dp"
        android:layout_height="50dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="705dp"

        android:background="@drawable/border_round_blue_white"
        android:fontFamily="@font/nanumregular"
        android:text="길찾기"
        android:textColor="#000000"
        android:textSize="28sp"
        android:textStyle="bold"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/communityBtn"
        android:layout_width="170dp"
        android:layout_height="50dp"
        android:layout_marginStart="220dp"
        android:layout_marginTop="705dp"

        android:background="@drawable/border_round_blue_white"
        android:fontFamily="@font/nanumregular"
        android:text="커뮤니티"
        android:textColor="#000000"
        android:textSize="28sp"
        android:textStyle="bold"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <Button
        android:id="@+id/time"
        android:layout_width="379dp"
        android:layout_height="116dp"
        android:layout_marginTop="182dp"
        android:layout_marginStart="16dp"
        android:layout_centerInParent="true"

        android:onClick="onMoveButtonClick"

        android:background="@drawable/border_round_blue"
        android:contentDescription="최소시간을 찾는 버튼입니다."

        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="195dp"
        android:layout_marginStart="35dp"

        android:text="최단시간"
        android:textColor="#000000"
        android:textStyle="bold"

        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/time_time_set"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="220dp"
        android:layout_marginStart="35dp"

        android:text="이동시간:"
        android:textStyle="bold"
        android:textColor="#000000"

        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/time_distance_set"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="220dp"
        android:layout_marginStart="150dp"

        android:text="이동거리:"
        android:textColor="#000000"

        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/time_cost_set"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="220dp"
        android:layout_marginStart="265dp"

        android:text="이동비용: "
        android:textColor="#000000"

        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/time_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="250dp"
        android:layout_marginStart="40dp"

        android:textStyle="bold"
        android:textColor="#000000"

        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/time_distance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="250dp"
        android:layout_marginStart="160dp"

        android:textColor="#000000"

        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/time_cost"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="250dp"
        android:layout_marginStart="280dp"

        android:textColor="#000000"

        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/distance"
        android:layout_width="379dp"
        android:layout_height="116dp"
        android:layout_marginTop="340dp"
        android:layout_marginStart="16dp"
        android:layout_centerInParent="true"

        android:onClick="onMoveButtonClick"

        android:background="@drawable/border_round_blue"
        android:contentDescription="최단거리를 찾는 버튼입니다."

        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="352dp"
        android:layout_marginStart="35dp"

        android:text="최단거리"
        android:textColor="#000000"
        android:textStyle="bold"

        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/distance_time_set"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="378dp"
        android:layout_marginStart="35dp"

        android:text="이동시간:"
        android:textColor="#000000"

        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/distance_distance_set"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="378dp"
        android:layout_marginStart="150dp"

        android:text="이동거리:"
        android:textColor="#000000"
        android:textStyle="bold"

        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        />

    <TextView
        android:id="@+id/distance_cost_set"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="378dp"
        android:layout_marginStart="265dp"

        android:text="이동비용:"
        android:textColor="#000000"

        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        />

    <TextView
        android:id="@+id/distance_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="408dp"
        android:layout_marginStart="40dp"

        android:textColor="#000000"

        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/distance_distance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"

        android:textColor="#000000"
        android:textStyle="bold"

        android:layout_marginTop="408dp"
        android:layout_marginStart="160dp"/>

    <TextView
        android:id="@+id/distance_cost"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="408dp"
        android:layout_marginStart="280dp"

        android:textColor="#000000"

        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent" />


    <Button
        android:id="@+id/cost"
        android:layout_width="379dp"
        android:layout_height="116dp"
        android:layout_marginTop="498dp"
        android:layout_marginStart="16dp"
        android:layout_centerInParent="true"
        android:onClick="onMoveButtonClick"

        android:background="@drawable/border_round_blue"
        android:contentDescription="최소비용을 찾는 버튼입니다."

        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="510dp"
        android:layout_marginStart="35dp"

        android:text="최소비용"
        android:textColor="#000000"
        android:textStyle="bold"

        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/cost_time_set"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="536dp"
        android:layout_marginStart="35dp"

        android:text="이동시간:"
        android:textColor="#000000"

        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/cost_distance_set"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="536dp"
        android:layout_marginStart="150dp"

        android:text="이동거리:"
        android:textColor="#000000"

        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent" />


    <TextView
        android:id="@+id/cost_cost_set"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="536dp"
        android:layout_marginStart="265dp"

        android:text="이동비용:"
        android:textColor="#000000"
        android:textStyle="bold"

        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/cost_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="566dp"
        android:layout_marginStart="40dp"

        android:textColor="#000000"

        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/cost_distance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="566dp"
        android:layout_marginStart="160dp"

        android:textColor="#000000"

        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/cost_cost"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="566dp"
        android:layout_marginStart="280dp"

        android:textColor="#000000"
        android:textStyle="bold"

        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent" />




</androidx.constraintlayout.widget.ConstraintLayout>